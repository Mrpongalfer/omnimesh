# OMNIMESH Frontend Environment Configuration
# Backend API endpoints
VITE_API_BASE_URL=http://localhost:8080
VITE_GRPC_ENDPOINT=http://localhost:50052
VITE_NEXUS_CORE_URL=http://localhost:50053

# Development configuration
VITE_DEV_MODE=true
VITE_ENABLE_MOCK_DATA=false

# Security settings
VITE_ENABLE_AUTH=true
VITE_AUTH_PROVIDER=omnimesh

# Real-time features
VITE_ENABLE_WEBSOCKETS=true
VITE_WEBSOCKET_URL=ws://localhost:8080/ws

# Feature flags
VITE_ENABLE_TIGER_LILY=true
VITE_ENABLE_MINDFORGE=true
VITE_ENABLE_FABRIC_MAP=true
